local function ZKSHRZT_fake_script() -- Settingsandmorethings.Highlight color \
	local script = Instance.new('LocalScript', Settingsandmorethings)\
\
	local sliderFrame = script.Parent -- The Frame containing the slider\
	local sliderButton = sliderFrame:WaitForChild("TextButton") -- The draggable button\
	local highlightColor = Color3.new(1, 0, 0) -- Default red color\
	\
	-- Function to update the highlight color based on slider position\
	local function updateHighlightColor()\
		local positionX = sliderButton.Position.X.Scale\
		local newColor = Color3.fromHSV(positionX, 1, 1) -- Convert position to color using HSV\
		highlightColor = newColor\
	\
		-- Update the color of the highlight for all players\
		for _, player in ipairs(game.Players:GetPlayers()) do\
			if player.Character then\
				local highlight = player.Character:FindFirstChild("Highlight")\
				if highlight then\
					highlight.FillColor = highlightColor\
					highlight.OutlineColor = highlightColor\
				end\
			end\
		end\
	end\
	\
	-- Function to handle dragging the slider button\
	local function onInputBegan(input)\
		if input.UserInputType == Enum.UserInputType.MouseButton1 then\
			local inputConnection\
			local dragConnection\
			local releaseConnection\
	\
			-- Dragging function\
			dragConnection = input.Changed:Connect(function()\
				if input.UserInputState == Enum.UserInputState.End then\
					dragConnection:Disconnect()\
					releaseConnection:Disconnect()\
				end\
			end)\
	\
			releaseConnection = game:GetService("UserInputService").InputChanged:Connect(function(moveInput)\
				if moveInput.UserInputType == Enum.UserInputType.MouseMovement then\
					local relativeX = (moveInput.Position.X - sliderFrame.AbsolutePosition.X) / sliderFrame.AbsoluteSize.X\
					sliderButton.Position = UDim2.new(math.clamp(relativeX, 0, 1), 0, sliderButton.Position.Y.Scale, sliderButton.Position.Y.Offset)\
					updateHighlightColor()\
				end\
			end)\
		end\
	end\
	\
	sliderButton.InputBegan:Connect(onInputBegan)\
	\
	\
end\
