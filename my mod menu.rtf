local function AYDOOWM_fake_script() -- InvisibilityButton.InvisibilityToggleScript \
	local script = Instance.new('LocalScript', InvisibilityButton)\
\
	local button = script.Parent\
	local invisible = false\
	local player = game.Players.LocalPlayer\
	local remoteEvent = game.ReplicatedStorage:WaitForChild("ToggleInvisibility")\
	\
	local function onInvisibilityToggled()\
		invisible = not invisible\
		-- This sends the event to the server, telling it to make the player invisible or visible\
		remoteEvent:FireServer(invisible)\
		-- Update the button text\
		button.Text = invisible and "Become Visible" or "Become Invisible"\
	end\
	\
	-- Connect the button click to the invisibility toggle function\
	button.MouseButton1Click:Connect(onInvisibilityToggled)\
	\
	-- Reset invisibility state when the character respawns\
	player.CharacterAdded:Connect(function()\
		invisible = false\
		button.Text = "Become Invisible"\
	end)\
	\
end\
